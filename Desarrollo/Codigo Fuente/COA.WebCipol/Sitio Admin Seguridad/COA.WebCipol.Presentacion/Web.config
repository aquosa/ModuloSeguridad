			</setting>
			<setting name="wsCipolServices_Fachada_Seguridad_Downloader_wsSIRActualizaciones" serializeAs="String">
				<value>http://localhost:59859/wsSIRActualizaciones.asmx</value>
			</setting>
		</wsCipolServices.Properties.Settings>
